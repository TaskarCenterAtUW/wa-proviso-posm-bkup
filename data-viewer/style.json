{
    "version": 8,
    "name": "OpenMapTiles - Basic",
    "sources": {
      "complete_openmaptiles": {
        "type": "vector",
        "tiles": [
          "http://localhost:3000/complete_openmaptiles/{z}/{x}/{y}"
        ],
        "minzoom": 0,
        "maxzoom": 14
      }
    },
    "layers": [
      {
        "id": "background",
        "type": "background",
        "paint": {
          "background-color": "#f8f4f0"
        }
      },
      {
        "id": "landcover",
        "type": "fill",
        "source": "complete_openmaptiles",
        "source-layer": "landcover",
        "paint": {
          "fill-color": "#d8e8c8",
          "fill-opacity": 0.6
        }
      },
      {
        "id": "landuse",
        "type": "fill",
        "source": "complete_openmaptiles",
        "source-layer": "landuse",
        "paint": {
          "fill-color": "#c8d8b8",
          "fill-opacity": 0.5
        }
      },
      {
        "id": "water",
        "type": "fill",
        "source": "complete_openmaptiles",
        "source-layer": "water",
        "paint": {
          "fill-color": "#a0c8f0",
          "fill-opacity": 0.6
        }
      },
      {
        "id": "waterway",
        "type": "line",
        "source": "complete_openmaptiles",
        "source-layer": "waterway",
        "paint": {
          "line-color": "#88aaff",
          "line-width": 1.2
        }
      },
      {
        "id": "boundary",
        "type": "line",
        "source": "complete_openmaptiles",
        "source-layer": "boundary",
        "paint": {
          "line-color": "#ff6600",
          "line-width": 1
        }
      },
      {
        "id": "transportation",
        "type": "line",
        "source": "complete_openmaptiles",
        "source-layer": "transportation",
        "paint": {
          "line-color": "#cccccc",
          "line-width": 1
        }
      },
      {
        "id": "transportation_name",
        "type": "symbol",
        "source": "complete_openmaptiles",
        "source-layer": "transportation_name",
        "layout": {
          "text-field": [
            "get",
            "name"
          ],
          "text-size": 10
        },
        "paint": {
          "text-color": "#666",
          "text-halo-color": "#ffffff",
          "text-halo-width": 1
        }
      },
      {
        "id": "poi",
        "type": "symbol",
        "source": "complete_openmaptiles",
        "source-layer": "poi",
        "layout": {
          "text-field": [
            "get",
            "name"
          ],
          "text-size": 11
        },
        "paint": {
          "text-color": "#444",
          "text-halo-color": "#fff",
          "text-halo-width": 1
        }
      },
      {
        "id": "housenumber",
        "type": "symbol",
        "source": "complete_openmaptiles",
        "source-layer": "housenumber",
        "layout": {
          "text-field": [
            "get",
            "housenumber"
          ],
          "text-size": 10
        },
        "paint": {
          "text-color": "#444",
          "text-halo-color": "#fff",
          "text-halo-width": 1
        }
      }
    ]
  }
  